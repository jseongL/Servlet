<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>Servlet</display-name>
 
 	<servlet>
 		<servlet-name>hello</servlet-name>
 		<servlet-class>com.marondal.hello.helloController</servlet-class>
 	</servlet>
 	
 	<servlet-mapping>
 		<servlet-name>hello</servlet-name>
 		<url-pattern>/hello</url-pattern>
 	</servlet-mapping>
 	
 	<servlet>
 		<servlet-name>ex01</servlet-name>
 		<servlet-class>com.marondal.servlet.ex.Ex01Controller</servlet-class>
 	</servlet>
 	
 	<servlet-mapping>
 		<servlet-name>ex01</servlet-name>
 		<url-pattern>/ex01</url-pattern>
 	</servlet-mapping>
 
 
 
 	<servlet>
 		<servlet-name>test01</servlet-name>
 		<servlet-class>com.marondal.servlet.test.test01</servlet-class>
 	</servlet>
 	
 	<servlet-mapping>
 		<servlet-name>test01</servlet-name>
 		<url-pattern>/servlet/test01</url-pattern>
 	</servlet-mapping>
 	
 	
</web-app>